{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-dashboard.html",
    "src/my-edit-student.html",
    "src/my-list-resources.html",
    "src/my-new-resource.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "index.html",
    "src/my-app.html",
    "src/my-dashboard.html",
    "src/my-edit-student.html",
    "src/my-list-resources.html",
    "src/my-new-resource.html",
    "src/my-view404.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "es5-bundled",
      "bundle": true,
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      }
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}