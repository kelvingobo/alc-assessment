{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-dashboard.html",
    "src/my-edit-student.html",
    "src/my-list-resources.html",
    "src/my-new-resource.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "name": "es5-bundled",
      "bundle": true,
      "js": {"minify": true},
      "css": {"minify": true},
      "html": {"minify": true}
    }
  ]
}
